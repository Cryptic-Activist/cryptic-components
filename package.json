{
	"name": "cryptic-components",
	"version": "1.1.1",
	"description": "React components for the Cryptic Activist platform.",
	"author": "Davi Silva <davi.cunha.silva.ca@gmail.com>",
	"source": "src/index.ts",
	"main": "./dist/index.js",
	"bin": "./dist/index.js",
	"license": "MIT",
	"keywords": [
		"cryptic-components",
		"react components"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Cryptic-Activist/cryptic-components"
	},
	"homepage": "https://github.com/Cryptic-Activist/cryptic-components/#readme",
	"publishConfig": {
