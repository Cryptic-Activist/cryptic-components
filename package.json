{
  "name": "cryptic-components",
  "version": "1.0.1",
  "description": "React components for the Cryptic Activist platform.",
  "author": "Davi Silva <davi.cunha.silva.ca@gmail.com>",
  "source": "./src/index.ts",
  "main": "./dist/src/index.js",
  "bin": "./dist/src/index.js",
  "license": "MIT",
  "keywords": [
    "cryptic-components",
    "react componentst"
  ],
  "repository": {
    "type": "git",
